package innerclass;

public class Student2 {
	private String name;
	private Score score;
	
	public Student2(String name) {
		
	}
	
	
	class Score { //내부 클래스
		int eng;
		int math;
		
		public void showInfo() {
			System.out.println("이름 : " + name);
			System.out.println("영어 : " + eng);
			System.out.println("수학 : " + math);
		}
	}
}
